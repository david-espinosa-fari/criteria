Exports views
mysql -u user -p INFORMATION_SCHEMA --skip-column-names -e "select table_name from tables where table_type = 'VIEW' and table_schema = 'vPvportalHost'" | xargs mysqldump -u pvportaldev -pd3sarr0ll0 --set-gtid-purged=OFF --lock-tables=false dbgpm vPvportalHost > only_views.sql
